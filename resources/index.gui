  <mask id="hourTopLeft">
    <g transform="translate(0, 100%) rotate(7.125)">
      <rect x="0" y="-100%" width="100%" height="100%" />
    </g>
  </mask>

  <mask id="hourTopRight">
    <g transform="translate(100%, 100%) rotate(-7.125)">
      <rect x="-100%" y="-100%" width="100%" height="100%" />
    </g>
  </mask>
  
  <g id="hourHand" transform="translate(50%, 50%)">
    <g transform="translate(-10, -80)">
      <svg mask="#hourTopLeft" width="10" height="80">
        <rect x="0" y="50%" width="100%" height="50%" fill="#111111" />
        <rect x="0" y="0" width="100%" height="50%" fill="inherit" />
      </svg>
    </g>
    <g transform="translate(0, -80)">
      <svg mask="#hourTopRight" width="10" height="80">
        <rect x="0" y="50%" width="100%" height="50%" fill="#111111" />
        <rect x="0" y="0" width="100%" height="50%" fill="inherit" />
      </svg>
    </g>
