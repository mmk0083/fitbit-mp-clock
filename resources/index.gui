  </mask>

  <mask id="minuteTopRight">
    <g transform="translate(100%, 100%) rotate(-5.194)">
      <rect x="-100%" y="-100%" width="100%" height="100%" />
    </g>
  </mask>

  <mask id="minuteBottomRight">
    <g transform="translate(100%, 0) rotate(33.69)">
      <rect x="-100%" y="0" width="100%" height="100%" />
    </g>
  </mask>

  <mask id="minuteBottomLeft">
    <g transform="rotate(-33.69)">
      <rect x="0" y="0" width="100%" height="100%" />
    </g>
  </mask>
  
  <g id="minuteHand" transform="translate(50%, 50%)">
    <g transform="translate(-10, -110)">
      <svg mask="#minuteTopLeft" width="10" height="110">
        <rect x="0" y="60%" width="100%" height="40%" fill="#111111" />
        <rect x="0" y="0" width="100%" height="60%" fill="inherit" />
